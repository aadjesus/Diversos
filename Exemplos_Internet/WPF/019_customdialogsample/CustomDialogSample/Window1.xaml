    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="CustomDialogSample" Height="300" Width="300"
    >
    <StackPanel>
        <Button Margin="5" x:Name="btnOne" Content="Example One" />
        <Button Margin="5" x:Name="btnTwo" Content="Example Two" />
        <Button Margin="5" x:Name="btnThree" Content="Example Three" />

        <TextBlock Margin="10" x:Name="tbResults" Text="Click buttons to test dialog" />
    </StackPanel>
</Window>
