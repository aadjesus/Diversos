<Window x:Class="Ink_A_Mator.DuplicateDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Duplicate Warning" WindowStyle="ToolWindow" Width="547" Height="85">
  <StackPanel Name="Panel">
    <TextBlock Text="The next frame has ink it, what would you like to do?" TextAlignment="Center" />
    <StackPanel Orientation="Horizontal" Margin="0,0,0,0">
      <Button Tag="Insert" Content="Insert duplicate before next frame" Click="OnSubmit" Margin="5,5,5,5" />
      <Button Tag="Replace" Content="Replace next frame with duplicate frame" Click="OnSubmit" Margin="5,5,5,5" />
      <Button Tag="Cancel" Content="Cancel" Click="OnSubmit" Margin="5,5,5,5" />
    </StackPanel>
  </StackPanel>
</Window>